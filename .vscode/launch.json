    "version": "0.2.0",
    "configurations": [

        {
            "name": "my_profile",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome",
                "--web-renderer",
                "html",
            ]
        },
        {
            "name": "my_profile (profile mode)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "profile",
            "args": [
                "-d",
                "chrome",
                "--web-renderer",
                "html",
            ]
        },
        {
            "name": "my_profile (release mode)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "release",
            "args": [
                "-d",
                "chrome",
                "--web-renderer",
                "html",
            ]
        }
    ]
}